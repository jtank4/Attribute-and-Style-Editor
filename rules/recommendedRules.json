[
  {
    "url": "www.instagram.com/",
    "refresh": 1000,
    "notes": "This set of rules adds a volume bar and a time scrub bar to videos on instagram.com",
    "elements": [
      {
        "selector": "video",
        "setAttributes": {
          "controls": ""
        }
      },
      {
        "selector": "div > div",
        "hasStyles": {
          "top":"0px",
          "right":"0px",
          "bottom":"0px",
          "left":"0px",
          "position":"absolute"
        },
        "hasChildren": [
          "span"
        ],
        "setStyles": {
          "bottom": "40px"
        }
      },
      {
        "selector": "div:empty",
        "hasStyles": {
          "top":"0px",
          "right":"0px",
          "bottom":"0px",
          "left":"0px",
          "position":"absolute"
        },
        "setStyles": {
          "bottom": "40px"
        }
      }
    ]
  },
  {
    "url": "https://www.tumblr.com/video",
    "tryAgain": 1000,
    "notes": "This set of rules adds a volume bar and a time scrub bar to videos on tumblr.com",
    "elements": [
      {
        "selector": "video",
        "setAttributes": {
          "controls": "true",
          "preload": "auto",
          "poster": ""
        }
      },
      {
        "selector": "div.vjs-big-play-button",
        "setStyles": {
          "display": "none"
        }
      },
      {
        "selector": "div.vjs-control-bar",
        "setStyles": {
          "display": "none"
        }
      },
      {
        "selector": "div.vjs-poster",
        "setStyles": {
          "display": "none",
          "width": "0px"
        }
      }
    ]
  },
  {
    "url": "tumblr.com/",
    "notes": "Adds controls to audio on Tumblr including volume bar.",
    "elements": [
      {
        "selector": "div.audio-player > audio",
        "setAttributes": {
          "controls": "true"
        },
        "setStyles": {
          "display": "block",
          "position": "absolute",
          "top": "60px"
        }
      }
    ]
  }
]